{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "build:all": "vite build && npm run build:server",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "NODE_ENV=development ts-node --project tsconfig.server.json -r dotenv/config src/server.ts",
    "migrate": "NODE_ENV=development ts-node --project tsconfig.server.json -r dotenv/config migrations/migrate.ts",
    "migrate:create": "NODE_ENV=development ts-node --project migrations/tsconfig.json -r dotenv/config migrations/create.ts",
    "db:seed": "NODE_ENV=development ts-node --project migrations/tsconfig.json -r dotenv/config migrations/seed.ts",
    "db:reset": "NODE_ENV=development ts-node --project migrations/tsconfig.json -r dotenv/config migrations/reset.ts",
    "db:status": "NODE_ENV=development ts-node --project migrations/tsconfig.json -r dotenv/config migrations/status.ts"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.5",
    "@supabase/supabase-js": "^2.49.4",
    "docx": "^9.3.0",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.485.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.30.0",
    "stripe": "^14.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/express": "^4.17.21",
    "@types/file-saver": "^2.0.7",
    "@types/jspdf": "^2.0.0",
    "@types/node": "^20.11.24",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/stripe": "^8.0.417",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "openapi-types": "^12.1.3",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "terser": "^5.39.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}